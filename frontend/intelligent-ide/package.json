{
  "$schema": "http://json.schemastore.org/package",
  "name": "intelligent-ide",
  "displayName": "intelligent-ide",
  "description": "a course-aware intelligent VScode plugin",
  "publisher": "MillerShi",
  "version": "1.2.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/sustech-cs304/team-project-25spring-36.git"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onNotebook:*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intelligent-ide.collaborative.upload",
        "title": "Upload Collaborative File",
        "category": "Intelligent IDE: Collaborative",
        "icon": "$(cloud-upload)"
      },

      {
        "command": "intelligent-ide.collaborative.delete",
        "title": "Delete Collaborative Entry",
        "category": "Intelligent IDE: Collaborative",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.collaborative.list",
        "title": "List Collaborative Entries",
        "category": "Intelligent IDE: Collaborative",
        "icon": "$(list-unordered)"
      },
      {
        "command": "intelligent-ide.collaborative.download",
        "title": "Download Collaborative Entry",
        "category": "Intelligent IDE: Collaborative",
        "icon": "$(cloud-download)"
      },
      {
        "command": "intelligent-ide.collaborative.openFile",
        "title": "Open collaborative-entry",
        "category": "Intelligent IDE: Course",
        "icon": "$(file)"
      },
      {
        "command": "intelligent-ide.login",
        "title": "Login",
        "category": "Intelligent IDE",
        "icon": "$(account)"
      },
      {
        "command": "intelligent-ide.register",
        "title": "Register",
        "category": "Intelligent IDE",
        "icon": "$(add)"
      },
      {
        "command": "intelligent-ide.update",
        "title": "Update",
        "category": "Intelligent IDE",
        "icon": "$(edit)"
      },
      {
        "command": "intelligent-ide.logout",
        "title": "Logout",
        "category": "Intelligent IDE",
        "icon": "$(sign-out)"
      },
      {
        "command": "intelligent-ide.switchRole",
        "title": "Switch Role",
        "category": "Intelligent IDE",
        "icon": "$(person)"
      },
      {
        "command": "intelligent-ide.course.post",
        "title": "Create Course",
        "category": "Intelligent IDE: Course",
        "icon": "$(add)"
      },
      {
        "command": "intelligent-ide.course.delete",
        "title": "Delete Course",
        "category": "Intelligent IDE: Course",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.course.openFile",
        "title": "Open Course File",
        "category": "Intelligent IDE: Course",
        "icon": "$(file)"
      },
      {
        "command": "intelligent-ide.course.join",
        "title": "Join Course",
        "category": "Intelligent IDE: Course",
        "icon": "$(debug-step-into)"
      },
      {
        "command": "intelligent-ide.collaborative.join",
        "title": "Join Collaborative Entry",
        "category": "Intelligent IDE: Course",
        "icon": "$(debug-step-into)"
      },
      {
        "command": "intelligent-ide.directory.delete",
        "title": "Delete Directory",
        "category": "Intelligent IDE: Course",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.directory.post",
        "title": "Create Directory",
        "category": "Intelligent IDE: Course",
        "icon": "$(new-folder)"
      },
      {
        "command": "intelligent-ide.course.chat",
        "title": "Join Course Chat",
        "category": "Intelligent IDE: Course",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "intelligent-ide.entry.delete",
        "title": "Delete File",
        "category": "Intelligent IDE: Course",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.entry.upload",
        "title": "Upload File",
        "category": "Intelligent IDE: Course",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "intelligent-ide.entry.move",
        "title": "Move File/Directory",
        "category": "Intelligent IDE: Course",
        "icon": "$(arrow-right)"
      },
      {
        "command": "intelligent-ide.student.delete",
        "title": "Remove Student",
        "category": "Intelligent IDE: Course",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.course.assignment.create",
        "title": "Create Assignment",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(add)"
      },
      {
        "command": "intelligent-ide.course.assignment.update",
        "title": "Update Assignment",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(edit)"
      },
      {
        "command": "intelligent-ide.course.assignment.delete",
        "title": "Delete Assignment",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.course.assignment.viewSubmissions",
        "title": "View Submissions for Assignment",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(list-ordered)"
      },
      {
        "command": "intelligent-ide.course.assignment.submit",
        "title": "Submit to Assignment",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "intelligent-ide.course.assignment.viewStatus",
        "title": "View My Assignment Statuses",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(checklist)"
      },
      {
        "command": "intelligent-ide.course.submission.grade",
        "title": "Grade Submission",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(check)"
      },
      {
        "command": "intelligent-ide.course.submission.delete",
        "title": "Delete Submission",
        "category": "Intelligent IDE: Assignment",
        "icon": "$(trash)"
      },
      {
        "command": "intelligent-ide.course.refresh",
        "title": "Refresh Course View",
        "category": "Intelligent IDE: Course",
        "icon": "$(refresh)"
      },
      {
        "command": "intelligent-ide.chat.open",
        "title": "Open AI Chat Assistant",
        "category": "Intelligent IDE: Chat",
        "icon": "$(comment-discussion)"
      },
      {
        "command": "intelligent-ide.chat.clear",
        "title": "Clear AI Conversation History",
        "category": "Intelligent IDE: Chat",
        "icon": "$(clear-all)"
      },
      {
        "command": "intelligent-ide.qna.open",
        "title": "Open QnA Interface",
        "category": "Intelligent IDE: QnA",
        "icon": "$(question)"
      },
      {
        "command": "intelligent-ide.course.viewAttachments",
        "title": "View Attachments",
        "category": "Intelligent IDE: course",
        "icon": "$(file-zip)"
      },
      {
        "command": "intelligent-ide.notebook.createForCourse",
        "title": "Create Notebook for Course",
        "category": "Intelligent IDE: Notebook",
        "icon": "$(new-file)"
      },
      {
        "command": "intelligent-ide.notebook.new",
        "title": "New Notebook",
        "category": "Intelligent IDE: Notebook",
        "icon": "$(new-file)"
      },
      {
        "command": "intelligent-ide.notebook.refresh",
        "title": "Refresh Notebook Explorer",
        "category": "Intelligent IDE: Notebook",
        "icon": "$(refresh)"
      },
      {
        "command": "intelligent-ide.notebook.exportToPdf",
        "title": "Export to PDF",
        "category": "Intelligent IDE: Notebook",
        "icon": "$(file-pdf)"
      },
      {
        "command": "intelligent-ide.notebook.delete",
        "title": "Delete Notebook",
        "category": "Intelligent IDE: Notebook",
        "icon": "$(trash)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "intelligent-ide",
          "title": "Intelligent IDE",
          "icon": "./src/resources/images/icon.svg"
        },
        {
          "id": "notebookView",
          "title": "Notebooks",
          "icon": "./src/resources/images/notebook-icon.svg"
        }
      ]
    },
    "views": {
      "intelligent-ide": [
        {
          "id": "loginView",
          "name": "Login View"
        },
        {
          "id": "courses",
          "name": "Course Explorer"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "intelligent-ide.login",
          "when": "view == loginView && !intelligent-ide.loggedIn",
          "group": "navigation"
        },
        {
          "command": "intelligent-ide.register",
          "when": "view == loginView && !intelligent-ide.loggedIn",
          "group": "navigation"
        },
        {
          "command": "intelligent-ide.update",
          "when": "view == loginView && intelligent-ide.loggedIn",
          "group": "navigation"
        },
        {
          "command": "intelligent-ide.logout",
          "when": "view == loginView && intelligent-ide.loggedIn",
          "group": "navigation"
        },
        {
          "command": "intelligent-ide.course.post",
          "when": "view == courses && intelligent-ide.loggedIn && intelligent-ide.userRole == 'teacher'",
          "group": "navigation"
        },
        {
          "command": "intelligent-ide.course.join",
          "when": "view == courses && intelligent-ide.loggedIn && intelligent-ide.userRole == 'student'",
          "group": "navigation"
        },
        {
          "command": "intelligent-ide.course.refresh",
          "when": "view == courses && intelligent-ide.loggedIn",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "intelligent-ide.course.delete",
          "when": "viewItem == course && intelligent-ide.loggedIn && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.directory.delete",
          "when": "viewItem == directory && intelligent-ide.loggedIn && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.directory.post",
          "when": "viewItem == course && intelligent-ide.loggedIn && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.course.chat",
          "when": "viewItem == course && intelligent-ide.loggedIn ",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.entry.delete",
          "when": "viewItem == entry-file || viewItem == entry-directory && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.collaborative.delete",
          "when": "viewItem == ICollaborativeEntry && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.collaborative.upload",
          "when": "viewItem == collaborative-folder && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.collaborative.download",
          "when": "viewItem == ICollaborativeEntry",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.collaborative.join",
          "when": "viewItem == ICollaborativeEntry",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.collaborative.openFile",
          "when": "viewItem == collaborative-entry",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.entry.upload",
          "when": "viewItem == directory || viewItem == entry-directory",
          "group": "2_workspace"
        },
        {
          "command": "intelligent-ide.entry.move",
          "when": "viewItem == entry-file",
          "group": "1_modification"
        },
        {
          "command": "intelligent-ide.student.delete",
          "when": "viewItem == student && intelligent-ide.loggedIn && intelligent-ide.userRole == 'teacher'",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.notebook.exportToPdf",
          "when": "viewItem == courseNotebookItem",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.notebook.delete",
          "when": "viewItem == courseNotebookItem",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == assignmentItem && intelligent-ide.userRole == 'teacher'",
          "command": "intelligent-ide.course.assignment.update",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == assignmentItem && intelligent-ide.userRole == 'teacher'",
          "command": "intelligent-ide.course.assignment.delete",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == assignmentFolder && viewItem != submissionItem && intelligent-ide.userRole == 'teacher'",
          "command": "intelligent-ide.course.assignment.create",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == submissionItem && intelligent-ide.userRole == 'teacher'",
          "command": "intelligent-ide.course.submission.grade",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == submissionItem && intelligent-ide.userRole == 'teacher'",
          "command": "intelligent-ide.course.submission.grade",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == assignmentItem && intelligent-ide.userRole == 'student'",
          "command": "intelligent-ide.course.assignment.submit",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == assignmentItem && intelligent-ide.userRole == 'student'",
          "command": "intelligent-ide.course.assignment.viewStatus",
          "group": "inline"
        },
        {
          "when": "view == courses && viewItem == submissionItem && intelligent-ide.userRole == 'teacher'",
          "command": "intelligent-ide.course.submission.delete",
          "group": "inline"
        },
        {
          "when": "viewItem == assignmentItem && view == courses",
          "command": "intelligent-ide.course.viewAttachments",
          "group": "inline"
        },
        {
          "when": "viewItem == submissionItem && view == courses",
          "command": "intelligent-ide.course.viewAttachments",
          "group": "inline"
        },
        {
          "command": "intelligent-ide.notebook.new",
          "when": "intelligent-ide.loggedIn && (viewItem == entry-directory || viewItem == directory) ",
          "group": "2_workspace"
        },
        {
          "command": "intelligent-ide.notebook.refresh",
          "when": " intelligent-ide.loggedIn && (viewItem == entry-directory || viewItem == directory) ",
          "group": "2_workspace"
        },
        {
          "command": "intelligent-ide.notebook.createForCourse",
          "when": "viewItem == course && intelligent-ide.loggedIn && intelligent-ide.userRole == 'teacher'",
          "group": "2_workspace"
        }
      ],
      "commandPalette": [
        {
          "command": "intelligent-ide.notebook.new",
          "when": "true",
          "group": "navigation"
        }
      ]
    },
    "notebooks": [
      {
        "type": "my-notebook",
        "displayName": "My Notebook",
        "selector": [
          {
            "filenamePattern": "*.ipynb"
          }
        ],
        "languages": [
          "javascript",
          "python",
          "java",
          "c",
          "markdown"
        ]
      }
    ],
    "walkthroughs": [
      {
        "id": "intelligent-ide-walkthrough",
        "title": "Getting Started with Intelligent IDE",
        "description": "Learn how to use the key features of Intelligent IDE for a productive learning experience.",
        "steps": [
          {
            "id": "user-registration",
            "title": "1. Register and Login",
            "description": "Create an account or log in to access all features of Intelligent IDE. \n\n[Register Now](command:intelligent-ide.register)",
            "media": {
              "markdown": "./src/resources/walkthrough/registration.md"
            }
          },
          {
            "id": "course-management",
            "title": "2. Manage Courses",
            "description": "View, join, and create courses based on your role.",
            "media": {
              "markdown": "./src/resources/walkthrough/course.md"
            }
          },
          {
            "id": "assignments",
            "title": "3. Complete Assignments",
            "description": "Submit and manage course assignments.",
            "media": {
              "markdown": "./src/resources/walkthrough/assignments.md"
            }
          },
          {
            "id": "ai-assistant",
            "title": "4. AI Chat Assistant",
            "description": "Get help with coding and learning concepts. \n\n[Open Chat](command:intelligent-ide.chat.open)",
            "media": {
              "markdown": "./src/resources/walkthrough/ai-chat.md"
            }
          },
          {
            "id": "collaborative-coding",
            "title": "5. Collaborative Coding",
            "description": "Work together with classmates in real-time. (Coming Soon)",
            "media": {
              "markdown": "./src/resources/walkthrough/collaborative.md"
            }
          },
          {
            "id": "custom-notebook",
            "title": "6. Interactive Notebooks",
            "description": "Create interactive notebooks with executable code. \n\n[Create Notebook](command:intelligent-ide.notebook.new)",
            "media": {
              "markdown": "./src/resources/walkthrough/notebook.md"
            }
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint ./src",
    "test": "vscode-test",
    "test:coverage": "nyc npm run test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.97.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "sinon": "^20.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@types/pdfkit": "^0.13.9",
    "axios": "^1.8.1",
    "form-data": "^4.0.2",
    "openai": "^4.103.0",
    "pdfkit": "^0.17.0",
    "quill": "^2.0.3",
    "showdown": "^2.1.0",
    "vsce": "^2.15.0",
    "ws": "^8.18.2",
    "y-quill": "^1.0.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  }
}